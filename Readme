crossgcc = x86_64-w64-mingw32-gcc



//copy EFI file to virtural image
mmd -i boot.img ::/EFI
mmd -i boot.img ::/EFI/BOOT
mcopy -i boot.img ./build/main.efi ::/EFI/BOOT/BOOTX64.EFI

//compile 
x86_64-w64-mingw32-gcc -Wall -Wextra -e efi_main -nostdinc -nostdlib -fno-builtin -Wl,--subsystem,10 -o main.efi main.c shell.c

//run qemu
qemu-system-x86_64 -bios /usr/share/edk2/x64/OVMF_CODE.fd -drive file=boot.img,format=raw -net none